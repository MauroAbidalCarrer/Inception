server 
{
	listen 443 ssl;

	server_name www.$DOMAIN_NAME $DOMAIN_NAME;

	ssl_certificate cert_path_placeholder;
	ssl_certificate_key /etc/ssl/private/nginx-selfsigned.key;

	ssl_protocols TLSv1.3;

	index index.html;
	root /var/www/html;

	location ~ [^/]\\.php(/|$) 
	{
        	try_files $uri =404;
	}
}
